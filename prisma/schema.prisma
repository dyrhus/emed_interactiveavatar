generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Demo {
  id           String   @id @default(cuid())
  customerName String
  introScript  String
  outroScript  String
  password     String?
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
