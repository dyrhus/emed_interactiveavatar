generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Demo {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  customerName String
  introScript  String
  outroScript  String
  password     String?
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt

  @@map("demos")
}
